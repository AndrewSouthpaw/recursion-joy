{
  "name": "recursion-joy",
  "version": "0.0.1",
  "description": "Recursion problems.",
  "repository": {
    "type": "git",
    "url": "git://github.com/hzhu/recursion-joy.git"
  },
  "author": "Henry Zhu <hz@henryzhu.me> (http://henryzhu.me)",
  "license": "MIT",
  "scripts": {
    "setup": "mkdir build && cp ./src/index.html ./build",
    "clean": "rimraf build coverage",
    "start": "webpack-dev-server --inline --hot --config ./internals/webpack/webpack.config.js --content-base build/",
    "test": "cross-env NODE_ENV=test karma start internals/testing/karma.conf.js --single-run",
    "test:watch": "npm run test -- --auto-watch --no-single-run"
  },
  "precommit": "test",
  "babel": {
    "presets": [
      "stage-0"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "dependencies": {
    "cross-env": "3.1.3"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-loader": "6.2.8",
    "babel-plugin-istanbul": "3.0.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "chai": "3.5.0",
    "coveralls": "2.11.15",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "mocha": "3.2.0",
    "pre-commit": "^1.1.3",
    "rimraf": "2.5.4",
    "sinon": "^1.17.6",
    "webpack": "2.1.0-beta.25"
  }
}
